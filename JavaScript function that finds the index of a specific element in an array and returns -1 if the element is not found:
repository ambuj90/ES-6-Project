function findIndex(arr, element) {
    for (let i = 0; i < arr.length; i++) {
        if (arr[i] === element) {
            return i;
        }
    }
    return -1;
}

// Example usage:
const arr = [10, 20, 30, 40, 50];
let element = 30;
console.log(findIndex(arr, element)); // Output: 2

element = 60;
console.log(findIndex(arr, element)); // Output: -1
